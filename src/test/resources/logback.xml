<configuration debug="true">

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{30} - %msg%n
            </pattern>
        </encoder>
    </appender>


    <logger name="com.tangqiang" level="debug"/>

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>


</configuration>
